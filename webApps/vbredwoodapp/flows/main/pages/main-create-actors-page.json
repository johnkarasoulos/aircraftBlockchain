      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-validation-group": {
        "path": "ojs/ojvalidationgroup"
